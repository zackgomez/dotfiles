! Urxvt
URxvt.buffered: true
URxvt.background: #1A1A1A
URxvt.foreground: #A9A9A9
URxvt.cursorColor: #5E468C
URxvt.geometry: 80x24
URxvt.transparent: 0
URxvt.internalBorder: 14
URxvt.title: Urxvt

! borderless and no scrollbar
URxvt*scrollBar_right: false
URxvt*scrollBar: false
URxvt*borderLess: false

! -- Fonts -- !
!URxvt.font:xft:envy code r:medium:pixelsize=12
!URxvt.boldFont:xft:envy code r:medium:pixelsize=12
!URxvt.font:xft:inconsolata:bold:pixelsize=12
URxvt.font:xft:dina:pixelsize=12
URxvt.boldFont:xft:dina:pixelsize=12
!URxvt.font:     -*-dina-medium-r-*-*-*-80-*-*-*-*-*-*
!URxvt.boldFont: -*-dina-medium-r-*-*-*-80-*-*-*-*-*-*
!URxvt.font:     -*-terminus-medium-r-*-*-16-*-*-*-*-*-*-*
!URxvt.boldFont: -*-terminus-medium-r-*-*-16*-*-*-*-*

